/* PLAYER_SETUP: Configures how players are positioned and global parameters */
<PLAYER_SETUP>
direct_placement
ai_info_map_type CUSTOM 0 0 0  			/* Tells AI: MapType=CUSTOM, IsNomad=0, IsMichi=0, ShowType=0 */
behavior_version 1						/* Used for versioning changes that might affect how existing maps generate */

/* LAND_GENERATION: Creates large terrain areas including player starting areas */
<LAND_GENERATION>
base_terrain LAYER_A						/* Sets default terrain covering entire map */

/* Bay water area */
create_land 
{
	terrain_type 				VODA		/* Terrain type for this create_land */
	land_percent 					100		/* Percent of map taken up by this create_land */
	land_position 					50 60	/* Land origin location in X Y percent value */
	left_border 					24		/* Left border for extent of this land in percent */
	right_border 					24		/* Right border for extent of this land in percent */
	top_border 						-24		/* Top border for extent of this land in percent */
	bottom_border 					24		/* Bottom border for extent of this land in percent */
	border_fuzziness 				100		/* Border adherence 0-100 */
	land_id							10		/* Land ID for this create_land */	
	zone 							10		/* Zone for this create_land */
}
/* Land terrain patching */
create_land 
{
	terrain_type 				LAYER_A		/* Terrain type for this create_land */
	left_border 					0		/* Left border for extent of this land in percent */
	right_border 					0		/* Right border for extent of this land in percent */
	top_border 						70		/* Top border for extent of this land in percent */
	bottom_border 					0		/* Bottom border for extent of this land in percent */
	land_id 						1		/* Land ID for this create_land */	
	zone 							1		/* Zone for this create_land */
}
create_land 
{
	terrain_type 				LAYER_A		/* Terrain type for this create_land */
	left_border 					0		/* Left border for extent of this land in percent */
	right_border 					75		/* Right border for extent of this land in percent */
	top_border 						0		/* Top border for extent of this land in percent */
	bottom_border 					0		/* Bottom border for extent of this land in percent */
	land_id 						1		/* Land ID for this create_land */	
	zone 							1		/* Zone for this create_land */
}
create_land 
{
	terrain_type 				LAYER_A		/* Terrain type for this create_land */
	left_border 					75		/* Left border for extent of this land in percent */
	right_border 					0		/* Right border for extent of this land in percent */
	top_border 						0		/* Top border for extent of this land in percent */
	bottom_border 					0		/* Bottom border for extent of this land in percent */
	land_id 						1		/* Land ID for this create_land */	
	zone 							1		/* Zone for this create_land */
}
/* Island generation */
create_land 
{
	generate_mode 					1		/* Generate mode to 1 to for random generation positioning */
	terrain_type 				LAYER_A		/* Terrain type for this create_land */
	number_of_tiles 		(LENGTH * 16)	/* Amount of tiles for this create_land */
	land_position 					50 25	/* Land origin location in X Y percent value */
	left_border 					36		/* Left border for extent of this land in percent */
	right_border 					36		/* Right border for extent of this land in percent */
	top_border 						8		/* Top border for extent of this land in percent */
	bottom_border 					36		/* Bottom border for extent of this land in percent */
	border_fuzziness 				100		/* Border adherence 0-100 */
	land_id							11		/* Land ID for this create_land */	
	zone 							11		/* Zone for this create_land */
	other_zone_avoidance_distance 	8		/* Distance in tiles that this land avoids other zones */
	clumping_factor 				10		/* Preference for tiles of the same clump to clump together 0-25+ */
}
